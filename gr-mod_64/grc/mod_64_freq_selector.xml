<?xml version="1.0"?>
<block>
  <name>FHSS_mod_64</name>
  <key>mod_64_freq_selector</key>
  <category>[FHSS]</category>
  <import>import mod_64</import>
  <make>mod_64.freq_selector($samp_rate,$sensitivity)</make>
  <!-- Make one 'param' node for every Parameter you want settable from the GUI.
       Sub-nodes:
       * name
       * key (makes the value accessible as $keyname, e.g. in the make node)
       * type -->
   <param>
    <name>sampling_rate</name>
    <key>samp_rate</key>
    <type>float</type>
  </param>

  <param>
    <name>Modulation Sensitivity</name>
    <key>sensitivity</key>
    <type>float</type>
  </param>
  
<!-- Make one 'param' node for every Parameter you want settable from the GUI.
       Sub-nodes:
       * name
       * key (makes the value accessible as $keyname, e.g. in the make node)
       * type -->
  <sink>
    <name>in</name>
    <type>float</type>
  </sink>
  	
  <!-- Make one 'source' node per output. Sub-nodes:
       * name (an identifier for the GUI)
       * type
       * vlen
       * optional (set to 1 for optional inputs) -->
  <source>
    <name>out</name>
    <type>float</type>
  </source>
</block>
